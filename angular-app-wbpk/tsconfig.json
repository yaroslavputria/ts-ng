{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "noImplicitAny": true,
    "suppressImplicitAnyIndexErrors": true,
    "rootDir": "./src",
    "emitDecoratorMetadata": true,
    "sourceMap": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "typeRoots": [ "./node_modules/@types" ],
    "types": [ "jasmine", "node", "protractor"],
    "lib": [
            "es2015",
            "es5",
            "dom",
            "scripthost"
        ]
  },
  "exclude": [
    "node_modules"
  ]
}
